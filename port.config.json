{
  "room1": {
    "ports": [
      {
        "path": "/dev/ttyUSB5",
        "actions": [
          {
            "socketEvent": "box.open",
            "socketReply": "boxIsOpened",
            "cmd": "openBox",
            "name": "box",
            "displayName": "Ящик"
          }
        ],
        "name": "boxPort"
      },
      {
        "path": "/dev/ttyUSB6",
        "actions": [
          {
            "socketEvent": "door.open",
            "socketReply": "firstDoorIsOpened",
